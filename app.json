{
  "expo": {
    "name": "ExcellentHacks",
    "slug": "apkhacks",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hackintown.ExcellentHacks"
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hackintown.ExcellentHacks"
    },
    "updates": {
      "url": "https://u.expo.dev/cda19132-67d9-4d05-aabe-1f2cd8631d8e"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cda19132-67d9-4d05-aabe-1f2cd8631d8e"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "hackintown"
  }
}
