{
  "expo": {
    "name": "hackintownapk",
    "slug": "hackintown-apk",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "jsEngine": "hermes",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hackintown.hackintownapk"
    },
    "android": {
      "permissions": [
        "INTERNET"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hackintown.hackintownapk"
    },
    "updates": { 
      "url": "https://u.expo.dev/ed939bcf-bb06-4664-beab-3139c4fc5485"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "http://194.238.17.67"
      },
      "eas": {
        "projectId": "ed939bcf-bb06-4664-beab-3139c4fc5485"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "owner": "hackintown"
  }
}
